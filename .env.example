# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_key_here

# Embedding Provider (choose one, or omit both for no embeddings)
# OpenAI API Key - Priority 1
OPENAI_API_KEY=your_openai_api_key_here

# Google Gemini API Key - Priority 2 (used if OpenAI key is not provided)
# Get it from: https://ai.google.dev
GEMINI_API_KEY=your_gemini_api_key_here

# Bot Mode Configuration
# Use 'polling' for development (default)
# Use 'webhook' for production
BOT_MODE=polling

# For webhook mode only
WEBHOOK_URL=https://your-domain.com/webhook
PORT=3000
