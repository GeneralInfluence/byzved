    "express": "^5.1.0",
    "grammy": "^1.38.4",
    "openai": "^4.24.1",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
